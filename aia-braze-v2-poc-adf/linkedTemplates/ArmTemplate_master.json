					},
					"Currents_Synapse_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('Currents_Synapse_properties_typeProperties_connectionString_secretName')]"
					},
					"SHIR_Local_properties_typeProperties_host": {
						"value": "[parameters('SHIR_Local_properties_typeProperties_host')]"
