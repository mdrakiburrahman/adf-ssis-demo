					"SHIR_Local_properties_typeProperties_userId": {
						"value": "[parameters('SHIR_Local_properties_typeProperties_userId')]"
					},
					"adfmetadata_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('adfmetadata_properties_typeProperties_connectionString_secretName')]"
					}
				}
